{
  "id" : "helloscript",
  "name" : "helloscript",
  "resources" : {
    "client" : {
      "type" : "aggregating-filesystem",
      "config" : {
        "directory" : "/home/mwringe/Projects-2/liveoak/dist/target/liveoak/client"
      }
    },
    "auth" : {
      "type" : "keycloak",
      "config" : { }
    },
    "authz" : {
      "type" : "security",
      "config" : {
        "policies" : [ {
          "policyName" : "URIPolicy",
          "policyResourceEndpoint" : "/helloscript/uri-policy/authzCheck"
        }, {
          "policyName" : "ACLPolicy",
          "policyResourceEndpoint" : "/helloscript/acl-policy/authzCheck",
          "includedResourcePrefixes" : [ "/helloscript" ]
        } ]
      }
    },
    "uri-policy" : {
      "type" : "uri-policy",
      "config" : {
        "rules" : [ {
          "uriPattern" : "*",
          "requestTypes" : [ "*" ],
          "allowedUsers" : [ "*" ]
        } ]
      }
    },
    "acl-policy" : {
      "type" : "acl-policy",
      "config" : {
        "autoRules" : [ ]
      }
    },
    "storage" : {
      "type" : "mongo",
      "config" : {
        "db" : "helloscript",
        "servers" : [ {
          "host" : "localhost",
          "port" : 27017
        } ],
        "credentials" : [ ]
      }
    },
    scripts: {
        type: "scripts",
        config: {
                script-directory: "${application.dir}/scripts"
        }
    }
  }
}
